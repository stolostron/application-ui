{
  "rules": {
    "no-use-before-define": [ 0 ],
    "no-process-exit": [ 0 ],
    "quotes": [ 0 ],
    "no-unused-vars": [ 1, { "args": "none" } ],
    "no-shadow": [ 0 ],
    "curly": [ 0 ],
    "no-underscore-dangle": [ 0 ],
    "no-multi-spaces": [ 0 ],
    "no-trailing-spaces": [ 1 ],
    "no-mixed-spaces-and-tabs": [ 1 ],
    "camelcase": [ 0 ],
    "dot-notation": [ 0 ],
    "no-mixed-requires": [ 0 ],
    "no-undef": [ 2 ]
  },
  "env": {
    "node": true,
    "mocha": true
  }
}

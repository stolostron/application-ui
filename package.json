  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node app.js",
    "start:production": "cross-env NODE_ENV=production node app.js",
    "clean": "del-cli dll public/*",
    "build:watch": "webpack --progress --watch",
    "build": "npm run clean && webpack --config webpack.dll.js --progress && webpack --progress",
    "build:production": "npm run build:dll:production && cross-env BUILD_ENV=production webpack",
    "build:dll:production": "cross-env BUILD_ENV=production webpack --config webpack.dll.js",
    "build:dll": "webpack --config webpack.dll.js --progress",
    "shrinkwrap:clean": "del-cli node_modules npm-shrinkwrap.json && npm install --no-optional && npm prune && npm shrinkwrap",
    "lint": "eslint --ext .jsx,.js .",
    "lint:fix": "eslint --ext .jsx,.js --fix .",
    "test:unit": "jest --colors",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "test:e2e": "nightwatch ./tests/e2e -e chrome.headless",
    "test:update-snapshot": "jest --updateSnapshot",
    "storybook": "start-storybook -p 6006 -s ./public,./node_modules/carbon-components/css",
    "build-storybook": "build-storybook",
    "test:chrome": "nightwatch ./tests/e2e -e chrome",
    "test:firefox": "nightwatch ./tests/e2e -e firefox",
    "test:safari": "nightwatch ./tests/e2e -e safari",
    "test:chrome.headless": "nightwatch ./tests/e2e -e chrome.headless",
    "test:firefox.headless": "nightwatch ./tests/e2e -e firefox.headless",
    "test:slack": "node tests/cypress/scripts/slack-reporter.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:unit -- -o"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylefmt",
      "git add"
    ]
  },
